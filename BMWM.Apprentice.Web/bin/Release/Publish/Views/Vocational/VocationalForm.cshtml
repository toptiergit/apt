                                Submit
                            </button>
                        </div>
                    </div>
                </div><!-- PAGE CONTENT ENDS -->
            </div><!-- /.col -->
        }

    </div><!-- /.row -->
</div>
@section Script {
    <!-- page specific plugin scripts -->
    <script src="@Url.Content("~/js/jquery.validate.min.js")"></script>
    <script type="text/javascript">
        $("#main-form").validate({
            rules: {
                // simple rule, converted to {required:true}
                name: "required",
                // compound rule
                '.radio-inline': {
                    required: true,
                    maxlength: 2
                },
                messages: {
                    '.radio-inline': {
                        required: "You must check at least 1 box",
                        maxlength: "Check no more than {0} boxes"
                    }
                },
                email: {
                    required: true,
                    email: true
                }
            }
        });
        $(() => {
            var nSafetyRank = parseInt($('[name="SafetyRank"]:checked').val());
            $("#Safety").val(nSafetyRank ? nSafetyRank:0);
            var nQualityRank = parseInt($('[name="QualityRank"]:checked').val());
            $("#Quality").val(nQualityRank ? nQualityRank:0);
            var nProJKnRank = parseInt($('[name="ProJKnRank"]:checked').val());
            $("#ProJKn").val(nProJKnRank ? nProJKnRank:0);
            var nTeamWorkRank = parseInt($('[name="TeamWorkRank"]:checked').val());
            $("#TeamWork").val(nTeamWorkRank ? nTeamWorkRank:0);
            var nPunctuallityRank = parseInt($('[name="PunctuallityRank"]:checked').val());
            $("#Punctuallity").val(nPunctuallityRank ? nPunctuallityRank:0);
        });
       
        $('input[type=radio][name=SafetyRank]').change(function () {
            var nSafetyRank = parseInt($('[name="SafetyRank"]:checked').val());
            $("#Safety").val(nSafetyRank);
            SUMTotalPoints();
        });
        $('input[type=radio][name=QualityRank]').change(function () {
            var nQualityRank = parseInt($('[name="QualityRank"]:checked').val());
            $("#Quality").val(nQualityRank);
            SUMTotalPoints();
        });
        $('input[type=radio][name=ProJKnRank]').change(function () {
            var nProJKnRank = parseInt($('[name="ProJKnRank"]:checked').val());
            $("#ProJKn").val(nProJKnRank);
            SUMTotalPoints();
        });
        $('input[type=radio][name=TeamWorkRank]').change(function () {
            var nTeamWorkRank = parseInt($('[name="TeamWorkRank"]:checked').val());
            $("#TeamWork").val(nTeamWorkRank);
            SUMTotalPoints();
        });
        $('input[type=radio][name=PunctuallityRank]').change(function () {
            var nPunctuallityRank = parseInt($('[name="PunctuallityRank"]:checked').val());
            $("#Punctuallity").val(nPunctuallityRank);
            SUMTotalPoints();
        });

        function SUMTotalPoints() {
            var nSafetyRank= parseInt($('[name="SafetyRank"]:checked').val());
            var nQualityRank = parseInt($('[name="QualityRank"]:checked').val());
            var nProJKnRank = parseInt($('[name="ProJKnRank"]:checked').val());
            var nTeamWorkRank = parseInt($('[name="TeamWorkRank"]:checked').val());
            var nPunctuallityRank= parseInt($('[name="PunctuallityRank"]:checked').val());
            var nSUMTotalPoints = (nSafetyRank ? nSafetyRank : 0 )+ (nQualityRank ? nQualityRank : 0 )+ (nProJKnRank ? nProJKnRank : 0 )+ (nTeamWorkRank ? nTeamWorkRank : 0 )+ (nPunctuallityRank ? nPunctuallityRank:0);
            $("#OverallTotalpoint").val(nSUMTotalPoints);
            var nRank = 0;
            if (nSUMTotalPoints < 8) {
                nRank = 1;//5-8
            } else if (nSUMTotalPoints < 13) {
                nRank = 2;//8-12
            } else if (nSUMTotalPoints < 18) {
                nRank = 3;//13-17
            } else if (nSUMTotalPoints < 23) {
                nRank = 4;//18-22
            } else if (nSUMTotalPoints >= 23) {
                nRank = 5;//23-25
            }
            $("input[name=OverallRatingPointSystem][value=" + nRank + "]").prop('checked', true);
        }
      
    </script>
}


