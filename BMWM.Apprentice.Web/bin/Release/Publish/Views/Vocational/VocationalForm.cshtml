                                                    @Html.RadioButtonFor(i => i.QualityRank, 2)2
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.QualityRank, 3)3
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.QualityRank, 4)4
                                                </label>
                                                <label class="radio-inline">
