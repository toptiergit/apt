                                                            <label class="radio-inline">
                                                                @Html.RadioButtonFor(i => i.OverallRatingPointSystem, 3) 13-17 Point
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="col-sm-9 control-label no-padding-right">
                                                            Performance exceeds expectations/ requirements.
                                                        </div>
                                                        <div class="col-sm-2">
                                                            <label class="radio-inline">
                                                                @Html.RadioButtonFor(i => i.OverallRatingPointSystem, 4) 18-22 Point
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="col-sm-9 control-label no-padding-right">
                                                            Performance significantly exceeds expectations/ requirements.
                                                        </div>
                                                        <div class="col-sm-2">
                                                            <label class="radio-inline">
                                                                @Html.RadioButtonFor(i => i.OverallRatingPointSystem, 5) 23-25 Point
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title">
                                    Student Comment
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right"></label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.StudentComments, new { htmlAttributes = new { @class = "input-xxlarge input-sm", @type = "text", rows = "6", placeholder = "" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-1" style="padding-left:0">
