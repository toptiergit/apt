@model ApplicationForm.Models.Vocational
@{
    ViewBag.Title = "Vocational Form";
    ViewBag.ActiveMenuId = "Vocational";
    var lang = Toptier.Web.Resource.ResourceManager.LanguageResource;
}
<div class="page-header">
    <h1>
        Vocational Student Performance Form
    </h1>
</div>
@section Style {
    <style type="text/css">
        .txt-dte {
            padding: 2px !important;
            height: 24px;
            width: 80px;
            font-size: 12px !important;
            text-align: center;
        }

        .breadcrumbs.sitemap {
            display: none;
        }

        #divBtnAction {
            display: none;
        }

        .modal-body {
            height: 500px;
            overflow-y: scroll;
        }
    </style>
}
<div class="row">
    <div class="col-xs-12">
        @using (Html.BeginForm("VocationalForm", "Vocational", FormMethod.Post, new { @id = "main-form", @class = "form-horizontal", @role = "form", @enctype = "multipart/form-data" }))
        {
            <div class="row">
                <div class="col-xs-12">
                    <!-- PAGE CONTENT BEGINS -->
                    <div class="col-xs-12 col-sm-12">

                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title"></h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="form-horizontal" role="form">
                                        <div class="form-group">
                                            @Html.EditorFor(model => model.Quarter, new { htmlAttributes = new { @type = "hidden" } })
                                            <label class="col-sm-3 control-label no-padding-right"> Name – Surname: </label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.FullName, new { htmlAttributes = new { @class = "input-large", @type = "text", placeholder = "Name", @readonly = "readonly" } })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right"> Emp. No:</label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.UserId, new { htmlAttributes = new { @class = "input-large input-sm", @type = "text", placeholder = "", @readonly = "readonly" } })
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">Hire date: </label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.HireDate, "{0:yyyy-MM-dd}", new { htmlAttributes = new { @class = "input-sm", @type = "date", placeholder = "" } })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">Position Title: </label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.PositionTitle, new { htmlAttributes = new { @class = "input-xxlarge", @type = "text", placeholder = "", @readonly = "readonly" } })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">Cost Center:</label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.CostCenter, new { htmlAttributes = new { @class = "input-xxlarge", @type = "text", placeholder = "", @readonly = "readonly" } })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">Review Period:</label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.ReviewPeriod, new { htmlAttributes = new { @class = "input-xxlarge", @type = "text", placeholder = "", @readonly = "readonly" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title">
                                    @*  การประเมินผลการปฏิบัติงานโดยผู้จัดการกระบวนการ (Performance evaluation by Process Manager)*@
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-9 control-label no-padding-right">
                                                @*Poor Performance: Received a formal corrective action for safety. = 1 <br>
                                                    Requires Improvement:Received an NOC for safety.= 2 <br>
                                                    Meets Expectations = 3 <br>
                                                    Exceeds Expectations: Safety exceeds expectations/requirements = 4 <br>
                                                    Exemplary Performance: Safety significantly exceeds expectations/requirements.= 5*@
                                            </label>

                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">
                                                1. Safety:
                                            </label>
                                            <div class="col-sm-9">
                                                <textarea class="form-control align-left" rows="2" readonly>Evaluate adherence to safety rules and guidelines, use of protective equipment, etc. Examine the degree of involvement in safety improvements.</textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-3 control-label"></div>
                                            <div class="col-sm-3 control-label no-padding-left">ระดับผลงาน(Performance rating) :</div>
                                            <div class="col-sm-4">
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.SafetyRank, 1)1
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.SafetyRank, 2)2
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.SafetyRank, 3)3
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.SafetyRank, 4)4
                                                </label>
                                                <label class="radio-inline">
                                                    @Html.RadioButtonFor(i => i.SafetyRank, 5)5
                                                </label>
                                                @Html.ValidationMessageFor(model => model.SafetyRank, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right"></label>
                                            <div class="col-sm-9">
                                                @Html.EditorFor(model => model.SafetyRecord, new { htmlAttributes = new { @class = "input-xxlarge", @type = "text", placeholder = "บันทึก(Comment)" } })
                                            </div>
                                        </div>

                                        <div class="hr hr-24"></div>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label no-padding-right">
                                                2. Quality:
                                            </label>
                                            <div class="col-sm-9">
                                                <textarea class="form-control align-left" rows="2" readonly>Evaluate accuracy, thoroughness and appearance of work produced compared to the requirements of the job. Examine the degree of involvement in improving quality.</textarea>

                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-3 control-label"></div>
                                            <div class="col-sm-3 control-label no-padding-left">ระดับผลงาน(Performance rating) :</div>
